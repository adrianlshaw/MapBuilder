<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="fill_parent"
    android:orientation="vertical" android:layout_width="fill_parent">
    <ProgressBar android:id="@+id/builder_progress_bar" android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_gravity="center_horizontal" android:layout_margin="10dip"></ProgressBar>
    <TextView android:layout_width="wrap_content" android:text="GPS Status: Stopped" android:id="@+id/builder_lastlocation" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:layout_margin="5dip" android:textStyle="bold" android:textSize="24dip"></TextView>
    <Button android:layout_width="fill_parent" android:id="@+id/button1" android:layout_height="wrap_content" android:layout_gravity="center_horizontal" android:text="@string/builder_start_btn"></Button>
    <Button android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/button3" android:text="@string/builder_console_addplace_button"></Button>
    <Button android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/forceAddPoint" android:text="@string/builder_force_add"></Button>
    <Button android:id="@+id/button2" android:layout_gravity="center_horizontal" android:layout_height="wrap_content" android:layout_width="fill_parent" android:text="@string/builder_stop_btn"></Button>
    <TextView android:layout_height="wrap_content" android:layout_width="wrap_content" android:id="@+id/nodeCount" android:text="Node count:"></TextView>
    <TextView android:id="@+id/textView2" android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="@string/builder_console_title" android:textStyle="bold" android:layout_marginTop="10dip"></TextView>
    <TextView android:text="" android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/console" ></TextView>
</LinearLayout>